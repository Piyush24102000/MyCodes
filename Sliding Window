WAP to find all the subarray whose length is 9
//find subarray whose sum is 9
let arr = [2,5,2,1,6,8,4]
let i = 0
let j = 0
let sum = 0 //2
let target = 9
let temp = []
while(j < arr.length){
  if(sum + arr[j] <= target){
    sum += arr[j]
    j++
   // console.log(sum,arr[j])
  } 
  else if( sum + arr[j] > target){
    sum -= arr[i]
    i++
  }  

  if(sum == target){
  temp.push(arr.slice(i,j))
  
  }
}
console.log(temp)
